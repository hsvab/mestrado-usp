mendeley-tags = {disserta{\c{c}}{\~{a}}o,envekhecimento,mobilidade,{\^{e}}xodo},
number = {9},
pages = {1674--1680},
title = {{{\^{E}}xodo seletivo, masculiniza{\c{c}}{\~{a}}o e envelhecimento da popula{\c{c}}{\~{a}}o rural na regi{\~{a}}o central do RS}},
url = {http://www.scielo.br/pdf/cr/v41n9/a10411cr3002.pdf},
volume = {41},
