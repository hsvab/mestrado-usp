@article{Dargay1999,
abstract = {This paper presents a new approach to the estimation of dynamic transport demand models by using information from independent samples of cross-sections of the population collected over time. A "pseudo-panel" approach is used to estimate a dynamic model of car ownership, based on data from annual Family Expenditure Surveys in the UK. The determinants of car ownership investigated include income, the costs of car ownership and use, public transport fares, and the socio-demographic characteristics of the households. The results obtained suggest that the applied methodology can provide a very fruitful basis for exploring the dynamics of household transport behaviour. /// Dieser Aufsatz pr{\"{a}}sentiert einen neuen Ansatz zur Sch{\"{a}}tzung dynamischer Transportnachfrage-Modelle, indem er Informationen unabh{\"{a}}ngiger Befragungen der Bev{\"{o}}lkerung nutzt, die im Laufe der Jahre durchgef{\"{u}}hrt wurden. Ein pseudo-panel Ansatz, basierend auf Daten der j{\"{a}}hrlichen "Family Expenditure Surveys" in Gro{\ss}britannien, wird verwendet, um ein dynamisches Autobesitzer-Modell zu sch{\"{a}}tzen. Die Determinanten der untersuchten Autobesitzer umfassen das Einkommen, die Kosten des Autobesitzes und der Nutzung, Fahrpreise {\"{o}}ffentlicher Verkehrsmittel und sozio-demographische Charakteristika der Haushalte. Die Ergebnisse lassen den Schlu{\ss} zu, da{\ss} die angewandte Methode eine sehr fruchtbare Basis sein kann, das dynamische Transportverhalten der Haushalte zu untersuchen. /// Cet article pr{\'{e}}sente une nouvelle approche de l'estimation des mod{\`{e}}les dynamiques de la demande en transport en utilisant les informations collect{\'{e}}es au cours du temps sur des {\'{e}}chantillons ind{\'{e}}pendants repr{\'{e}}sentatifs de populations. Une approche "pseudo-panel" est utilis{\'{e}}e pour {\'{e}}valuer un mod{\`{e}}le dynamique de possession de voitures bas{\'{e}} sur les donn{\'{e}}es du sondage annuel des d{\'{e}}penses familiales au Royaume-Uni. Les {\'{e}}l{\'{e}}ments d{\'{e}}terminants de la propri{\'{e}}t{\'{e}} automobile {\'{e}}tudi{\'{e}}s incluent le revenu, les co{\^{u}}ts de possession et d'utilisation de voitures, les tarifs du transport public et les caract{\'{e}}ristiques socio-d{\'{e}}mographiques des m{\'{e}}nages. Les r{\'{e}}sultats obtenus sugg{\`{e}}rent que la m{\'{e}}thodologie adopt{\'{e}}e peut fournir une base tr{\`{e}}s fructueuse pour explorer les {\'{e}}volutions du comportement des m{\'{e}}nages en mati{\`{e}}re de transport. /// Este art{\'{\i}}culo presenta una nueva manera de estimar modelos de demanda de transporte din{\'{a}}micos utilizando informaci{\'{o}}n de muestras independientes de secciones cruzadas de la poblaci{\'{o}}n recogidas a lo largo del tiempo. Se utiliza un enfoque pseudo-panel para estimar un modelo din{\'{a}}mico de tenencia de autom{\'{o}}viles con datos de las entrevistas de gasto familiar en el Reino Unido. Los determinantes de la tenencia de autom{\'{o}}viles investigados incluyen la renta, los costes de la tenencia y uso de autom{\'{o}}viles, los precios del transporte p{\'{u}}blico, y las caracter{\'{\i}}sticas socio-demogr{\'{a}}ficas de las familias. Los resultados obtenidos sugieren que la metodolog{\'{\i}}a aplicada puede ofrecer una base muy fruct{\'{\i}}fera para explorar la din{\'{a}}mica del comportamiento de transporte familiar. CR - Copyright {\&}{\#}169; 1999 The London School of Economics and Political Science and University of Bath},
author = {Dargay, Joyce M. and Vythoulkas, Petros C.},
journal = {Journal of Transport Economics and Policy},
number = {3},
pages = {287--302},
title = {{Estimation of a dynamic car ownership model: A pseudo-panel approach}},
volume = {33},
year = {1999}
}
