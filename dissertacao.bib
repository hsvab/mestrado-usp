issn = {1879-1646},
journal = {Journal of health economics},
keywords = {Alcohol demand,Cross price elasticities,Elasticities,Pseudo-panel},
month = {mar},
pages = {96--103},
pmid = {24508846},
publisher = {Elsevier B.V.},
title = {{Estimation of own and cross price elasticities of alcohol demand in the UK-A pseudo-panel approach using the Living Costs and Food Survey 2001-2009.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3991422{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {34},
