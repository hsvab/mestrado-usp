journal = {Transportation Research Part A: Policy and Practice},
keywords = {Barcelona,Local travel,Proximity,Walking},
pages = {210--222},
publisher = {Elsevier Ltd},
title = {{Walking short distances. The socioeconomic drivers for the use of proximity in everyday mobility in Barcelona}},
url = {http://www.sciencedirect.com/science/article/pii/S0965856414002419},
volume = {70},
year = {2014}
}
