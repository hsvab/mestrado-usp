@article{RAUBER2010,
abstract = {A configura{\c{c}}{\~{a}}o populacional vem sofrendo acentuadas transforma{\c{c}}{\~{o}}es ao longo da {\'{u}}ltima d{\'{e}}cada. Atualmente, a masculiniza{\c{c}}{\~{a}}o e o envelhecimento populacional s{\~{a}}o apontados como caracter{\'{\i}}sticas dessa realidade. Neste sentido, este trabalho teve como objetivo analisar a ocorr{\^{e}}ncia e o comportamento dos processos de masculiniza{\c{c}}{\~{a}}o e envelhecimento da popula{\c{c}}{\~{a}}o rural de 27 munic{\'{\i}}pios da Regi{\~{a}}o Central do Rio Grande do Sul, em diferentes faixas et{\'{a}}rias. Para alcan{\c{c}}ar esse prop{\'{o}}sito, utilizaram-se dados da Contagem Populacional de 1996 e 2007 do IBGE referentes {\`{a}} popula{\c{c}}{\~{a}}o rural regional sistematizados em quatro faixas: 0 a 14 anos, 15 a 24, 25 a 59 e 60 ou mais anos. As diferen{\c{c}}as entre os sexos foram submetidas ao Teste para Diferen{\c{c}}a entre Duas Propor{\c{c}}{\~{o}}es com 5{\%} de probabilidade de erro. Tamb{\'{e}}m foram calculadas a diferen{\c{c}}a percentual da popula{\c{c}}{\~{a}}o total e de cada sexo entre 1996 e 2007. O processo de masculiniza{\c{c}}{\~{a}}o rural consolida sua presen{\c{c}}a na Regi{\~{a}}o Central do RS, sendo a popula{\c{c}}{\~{a}}o adulta atingida com mais intensidade. A sobreposi{\c{c}}{\~{a}}o masculina significativa pode ser observada em todos os munic{\'{\i}}pios, aumentando da primeira a terceira faixa et{\'{a}}ria. Em alguns munic{\'{\i}}pios, houve uma intensifica{\c{c}}{\~{a}}o da masculiniza{\c{c}}{\~{a}}o rural tamb{\'{e}}m entre a popula{\c{c}}{\~{a}}o idosa. Tamb{\'{e}}m pode ser observado, de forma geral, um abrandamento do predom{\'{\i}}nio masculino entre a popula{\c{c}}{\~{a}}o jovem. A forte redu{\c{c}}{\~{a}}o da popula{\c{c}}{\~{a}}o de 0 a 14 anos, bem como o aumento da popula{\c{c}}{\~{a}}o idosa no per{\'{\i}}odo mostra um processo de envelhecimento entre a popula{\c{c}}{\~{a}}o estudada. Essa configura{\c{c}}{\~{a}}o populacional pode comprometer a sucess{\~{a}}o nos estabelecimentos rurais, interferindo na din{\^{a}}mica social e produtiva do espa{\c{c}}o rural da Regi{\~{a}}o Central do RS.},
author = {Froelich, Jos{\'{e}} Marcos and Rauber, Cassiane da Costa and Carpes, Ricardo Howes and Toebe, Marcos},
file = {:home/haydee/{\'{A}}rea de trabalho/Ref{\_}bib/Froelich et al.{\_}2011.pdf:pdf},
