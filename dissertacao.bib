journal = {Medicine and science in sports and exercise},
keywords = {Adult,Crime,Data Collection,Environment Design,Esthetics,Humans,Questionnaires,Recreation,Residence Characteristics,Residence Characteristics: classification,Safety,Walking},
month = {sep},
