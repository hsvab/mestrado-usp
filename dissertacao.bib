issn = {03861112},
journal = {International Association of Traffic and Safety Sciences},
keywords = {Attractiveness,Car,Policies,Psychological factors,Public transport},
number = {2},
pages = {27--35},
publisher = {International Association of Traffic and Safety Sciences},
title = {{Can Public Transport Compete With the Private Car?}},
url = {http://dx.doi.org/10.1016/S0386-1112(14)60141-2},
volume = {27},
year = {2003}
}
