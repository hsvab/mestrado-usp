pages = {79},
title = {{Contribui{\c{c}}{\~{o}}es ao Estudo de Implanta{\c{c}}{\~{a}}o de Ped{\'{a}}gio Urbano em S{\~{a}}o Paulo S{\~{a}}o Paulo}},
year = {2013}
}
